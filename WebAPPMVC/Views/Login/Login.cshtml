
@model WebAPPMVC.Models.UserLogin
@{
    ViewData["Title"] = "Login";
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <title>Login V3</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="~/images/icons/favicon.ico" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/extra/bootstrap/css/bootstrap.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/fonts/iconic/css/material-design-iconic-font.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/extra/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/extra/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/extra/animsition/css/animsition.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/extra/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/extra/daterangepicker/daterangepicker.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/css/main.css">
    <!--===============================================================================================-->
</head>
<body>

    <div class="limiter">
        <div class="container-login100" style="background-image: url('images/bg-01.jpg');">
            <div class="wrap-login100">
                <form  asp-action="Login" class="login100-form validate-form">
                    <span class="login100-form-logo">
                        <i class="zmdi zmdi-shield-security"></i>
                    </span>

                    <span class="login100-form-title p-b-34 p-t-27">
                        Log in
                    </span>

                    <div class="wrap-input100 validate-input" data-validate="Enter username">
                            <input asp-for="UserName" class="input100"  placeholder="Username"/>
                            <span asp-validation-for="UserName" class="focus-input100" data-placeholder="&#xf191;"></span>
                    </div>

                    <div class="wrap-input100 validate-input" data-validate="Enter password">
                        <input asp-for="UserPassword" class="input100" type="password" placeholder="Password" />
                        <span asp-validation-for="UserPassword" class="focus-input100" data-placeholder="&#xf191;"></span>
                    </div>

                    <div class="contact100-form-checkbox">
                        <input class="input-checkbox100" id="ckb1" type="checkbox" name="remember-me">
                        <label class="label-checkbox100" for="ckb1">
                            Remember me
                        </label>
                    </div>

                    <div class="container-login100-form-btn">
                        <button type="submit" class="login100-form-btn">
                            LOGIN
                        </button>
                    </div>

                    @*<div class="text-center p-t-90">
                            <a class="txt1" href="#">
                                Forgot Password?
                            </a>
                        </div>*@
                </form>
            </div>
        </div>
    </div>


    <div id="dropDownSelect1"></div>

    <!--===============================================================================================-->
    <script src="~/extra/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/extra/animsition/js/animsition.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/extra/bootstrap/js/popper.js"></script>
    <script src="~/extra/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/extra/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/extra/daterangepicker/moment.min.js"></script>
    <script src="~/extra/daterangepicker/daterangepicker.js"></script>
    <!--===============================================================================================-->
    <script src="~/extra/countdowntime/countdowntime.js"></script>
    <!--===============================================================================================-->
    <script src="~/js/main.js"></script>
    <script language="javascript">
    </script>
</body>
</html>




